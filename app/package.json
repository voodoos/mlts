{
  "name": "mlts",
  "version": "5.0.4",
  "description": "A simple application to experiment with the MLTS programming language.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . mlts --overwrite --asar=true --platform=win32 --arch=ia32 --icon=icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"MLTS\"",
    "package-linux": "electron-packager . mlts --overwrite --asar --platform=linux --arch=x64 --icon=icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://VooDooS@github.com/VooDooS/mlts.git"
  },
  "keywords": [
    "functional",
    "programming",
    "lambda",
    "prolog",
    "binders",
    "lambda",
    "calculus"
  ],
  "author": "Ulysse GÃ©rard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VooDooS/mlts/issues"
  },
  "homepage": "https://github.com/VooDooS/mlts#readme",
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-packager": "^13.1.1"
  },
  "resolutions": {
    "sshpk": "1.16.1"
  }
}